<p id="notice"><%= notice %></p>

<h1>Listing User Taxi Calls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_taxi_calls.each do |user_taxi_call| %>
      <tr>
        <td><%= link_to 'Show', user_taxi_call %></td>
        <td><%= link_to 'Edit', edit_user_taxi_call_path(user_taxi_call) %></td>
        <td><%= link_to 'Destroy', user_taxi_call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

